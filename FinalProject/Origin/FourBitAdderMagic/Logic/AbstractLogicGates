public class AbstractGate {
    private int mUsiIn1;
    private int mUsiIn2;
    private int mUsiOut;

    public void setIn1(int val) {
        mUsiIn1 = val;
        update();
    }

    public void setIn2(int val) {
        mUsiIn2 = val;
        update();
    }

    public int getOut() {
        return mUsiOut;
    }

    public int getIn1() {
        return mUsiIn1;
    }

    public int getIn2() {
        return mUsiIn2;
    }

    private void update() {
        // Implement the logic for the update here (if applicable)
        // For simplicity, this method is left empty in this example.
    }

    public String repr() {
        return "in1: " + mUsiIn1 + "\n\tin2: " + mUsiIn2 + "\n\t\tout: " + mUsiOut;
    }

    // Other methods and constructor for AbstractGate can be added here
}
