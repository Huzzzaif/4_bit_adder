public class LogicGates {
    // AND gate
    public static boolean and(boolean input1, boolean input2) {

        
        return input1 && input2;


    }

    // OR gate
    public static boolean or(boolean input1, boolean input2) {
        return input1 || input2;
    }

    // NAND gate
    public static boolean nand(boolean input1, boolean input2) {
        return !(input1 && input2);
    }

    // NOT gate derived from NAND gate
    public static boolean not(boolean input) {
        return nand(input, input);
    }

    public static void main(String[] args) {
        boolean a = true;
        boolean b = false;

        // Test the gates
        System.out.println("AND gate: " + and(a, b));   // Output: false
        System.out.println("OR gate: " + or(a, b));     // Output: true
        System.out.println("NAND gate: " + nand(a, b)); // Output: true
        System.out.println("NOT gate: " + not(a));      // Output: false
    }
}
